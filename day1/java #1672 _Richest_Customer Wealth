class Solution {
    public int maximumWealth(int[][] accounts) {
       int maxi = 0;
        for(int i=0; i<accounts.length; i++){
            int sum = 0;
            int []arr = accounts[i];
            for(int ele:arr) sum+=ele;
            maxi = Math.max(sum,maxi);   
        } 
        return maxi; 
    }
}
